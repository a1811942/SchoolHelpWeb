<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.FollowDao">


    <select id="getFollowByPointStudentId" resultType="com.example.demo.entity.Follow">
        select *
        from follow where follow_student_id=#{followStudentId};
    </select>
</mapper>